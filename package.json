{
  "name": "create-leeais-builder",
  "version": "1.0.1",
  "description": "A CLI tool to create projects with ease.",
  "license": "MIT",
  "author": "longdech",
  "homepage": "https://github.com/leeais/leeais-builder#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/leeais/leeais-builder.git"
  },
  "bugs": {
    "url": "https://github.com/leeais/leeais-builder/issues"
  },
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "create-leeais-builder": "dist/index.js"
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write \"**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,md}\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "prepublishOnly": "npm run type-check && npm run build",
    "test": "node dist/index.js test-project",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.12",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@eslint/js": "^9.39.1",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.2",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "prettier-package-json": "^2.8.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "keywords": [
    "cli",
    "create-leeais-builder",
    "generator",
    "react",
    "scaffold",
    "starter",
    "typescript",
    "vite"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "lint-staged": {
    "src/index.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "package.json": [
      "prettier-package-json --write"
    ]
  }
}
